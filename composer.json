{
    "name": "benatespina/stack-exchange-api-client",
    "description": "StackExchange v2.2 API client",
    "keywords": ["stackExchange", "api", "library", "client"],
    "homepage": "https://github.com/benatespina/StackExchangeApiClient",
    "version": "0.5.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Beñat Espiña",
            "email": "benatespina@gmail.com",
            "homepage": "http://benatespina.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "kriswallsmith/buzz": "0.10"
    },
    "require-dev": {
        "phpspec/phpspec": "2.1.x-dev",
        "henrikbjorn/phpspec-code-coverage": "1.0.x-dev",
        "satooshi/php-coveralls": "0.6",
        "squizlabs/php_codesniffer": "1.*",
        "phpmd/phpmd": "dev-master"
    },
    "autoload": {
        "psr-4": { 
            "BenatEspina\\StackExchangeApiClient\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "BenatEspina\\StackExchangeApiClient\\spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
