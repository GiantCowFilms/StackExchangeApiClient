{
    "name": "benatespina/stack-exchange-api-client",
    "description": "StackExchange v2.2 API client",
    "keywords": [
        "stackExchange",
        "api",
        "library",
        "client"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/benatespina/StackExchangeApiClient",
    "authors": [
        {
            "name": "Beñat Espiña",
            "email": "benatespina@gmail.com",
            "homepage": "https://benatespina.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.5",
        "phpspec/phpspec": "^4.0",
        "phpunit/phpunit": "^5.6"
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix --config=.php_cs",
            "php-cs-fixer fix --config=.phpspec_cs"
        ],
        "test": "phpspec run"
    },
    "autoload": {
        "psr-4": {
            "BenatEspina\\StackExchangeApiClient\\": "src/BenatEspina/StackExchangeApiClient/"
        }
    }
}
